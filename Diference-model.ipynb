import numpy as np
import matplotlib.pyplot as plt
from math import cosh

# --- 1. PARÂMETROS FINAIS DA LEI DE COERÊNCIA GEOMÉTRICA (PHI) ---
# OTIMIZADO PARA VISUALIZAÇÃO MAIS DRAMÁTICA

# AUMENTAMOS O ALPHA PARA QUE O EFEITO SEJA VISÍVEL NO GRÁFICO E MOSTRE O BOOST SIGNIFICATIVO
alpha_final = 0.5      # <<-- MUDANÇA AQUI: AUMENTADO PARA VISUALIZAÇÃO CLARA DO BOOST
beta_final = 1.18       # Fator de Decaimento (Derivado da curvatura k)
gamma_final = 11.92     # Redshift do Pico (z_turn - Onde o JWST exige o boost)

# Parâmetros de Densidade (Flat Universe: Omega_total = 1)
Omega_m0 = 0.31         # Matéria (Bariônica + CDM)
Omega_r0 = 9.2e-5       # Radiação
Omega_Lambda_final = 1.0 - Omega_m0 - Omega_r0 # ~0.69 (o valor standard)

# Parâmetros LCDM (Para Comparação - usa o mesmo Omega_Lambda_final para ser uma comparação justa)
Omega_L_LCDM = Omega_Lambda_final


# --- 2. FUNÇÕES DE EXPANSÃO (H(z)/H0) COERENTE VIA G_eff ---

def E_squared_coherent(z, alpha, beta, gamma, Omega_m0, Omega_r0, Omega_Lambda):
    """Calcula E^2(z) usando o Protocolo Phi na forma G_eff."""
    ln_term = beta * np.log(1 + z) - beta * np.log(1 + gamma)
    f_z = 1 / (np.cosh(ln_term) ** 2)
    
    Coherence_Term = 1 + alpha * f_z # G_eff(z)
    term_matter_rad = (Omega_m0 * (1 + z)**3 + Omega_r0 * (1 + z)**4) * Coherence_Term
    E2 = term_matter_rad + Omega_Lambda
    
    return E2

def E_coherent(z, *params):
    return np.sqrt(E_squared_coherent(z, *params))

def E_LCDM_pure(z):
    """Calcula E(z) para o modelo Lambda CDM de referência."""
    return np.sqrt(Omega_m0 * (1 + z)**3 + Omega_r0 * (1 + z)**4 + Omega_L_LCDM)


# --- 3. GERAÇÃO E PLOTAGEM ---

z_range = np.linspace(0.0, 30.0, 500) # Range maior para ver a evolução completa
params_phi_coherent = (alpha_final, beta_final, gamma_final, Omega_m0, Omega_r0, Omega_Lambda_final)

E_phi_coherent = E_coherent(z_range, *params_phi_coherent)
E_lcdm_pure = E_LCDM_pure(z_range)

# Plotagem das Proporções das Duas Teorias
plt.figure(figsize=(10, 6))
plt.plot(z_range, E_phi_coherent, label='Protocolo $\\phi$ ($G_{eff}$ Coerente)', color='darkblue', linewidth=3)
plt.plot(z_range, E_lcdm_pure, label='$\\Lambda$CDM (Referência)', linestyle='--', color='red', alpha=0.7)

plt.axvline(x=gamma_final, color='gray', linestyle=':', label=f'Pico Exigido ($z \\approx {gamma_final:.2f}$)')

plt.xlabel('Redshift (z)')
plt.ylabel('Taxa de Expansão Normalizada $E(z) = H(z) / H_0$')
plt.title('Comparação Direta da Taxa de Expansão: Protocolo $\\phi$ vs. $\\Lambda$CDM')
plt.yscale('log') # Escala logarítmica é essencial para ver as proporções em alto z
plt.grid(True, which="both", ls="--", alpha=0.5)
plt.xlim(0, 30)
plt.legend()
plt.show()
